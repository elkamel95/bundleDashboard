{
   
        "name": "generic/dashboard",
        "type": "symfony-bundle",
        "description": "  Api de gestion de tableau de bord sous la forme de widgets génériques  ",
        "keywords": ["framework"],
        "homepage": "https://symfony.com",
        "license": "MIT",
        "authors": [
            {
                "name": "Fahmi Elkamel",
                "email": "fahmielkamel@gmail.com"
            },
            {
                "name": "Symfony Community",
                "homepage": "https://symfony.com/contributors"
            }
        ],
    "require": {
        "php": "^7.0.8",
    },
     "require-dev": {
          "api-platform/api-pack": "^1.2"
  
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
  "autoload": {
        "psr-4": { "Generic\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Generic\\Tests\\": "tests/" }
    },
    "extra": {
       "symfony": {
            "allow-contrib": false,
            "require": "3.4.*"
        }
       
    }
 
  
}
